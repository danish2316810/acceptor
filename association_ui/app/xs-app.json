{
    "welcomeFile": "/project1/webapp/index.html",
    "authenticationMethod":"route",
    "routes": [
      {
        "source": "^/project1/webapp/index.html/(.*)$",
        "target": "$1",
        "localDir": ".",
        "cacheControl": "no-cache, no-store, must-revalidate",
        "authenticationType":"xsuaa"
      },
      
      {
        "source": "^/(.*)$",
        "target": "$1",
        "destination": "srv-api",
        "csrfProtection": false,
        "authenticationType":"xsuaa"
      }
    ]
  }
  